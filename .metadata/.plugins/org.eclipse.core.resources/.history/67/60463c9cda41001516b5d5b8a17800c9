import java.io.*;

public class LetterChange {

	public static void main(String[] args) {

		String s1;
		String s2 = "";
		String s3;
		try {
			s1 = getInput("Enter a string: ");
//			int a = 66;
//			char c = (char) a;
//			String str = Integer.toString(66);
//			System.out.println((char) str);
			for(char element: s1.toCharArray()){ 
				s2 += (char) (element + 1); }
			
			s3 = vowelHandler(s2);
			System.out.println(s3);
//			char[] s3 = s2.toCharArray();
//			System.out.println(s3[2]+1);
			
			
		} catch (IOException e) {
			e.printStackTrace();
		}
//				Str.toCharArray;
				
				
		}
	public static String getInput (String prompt) throws IOException {
				
		BufferedReader stdin = new BufferedReader(
		new InputStreamReader(System.in));
				
		System.out.print(prompt);
		System.out.flush();
				
		try {
			return stdin.readLine();
		}catch(Exception e){
			return "Error: " + e.getMessage();
		}
	}

	public static String vowelHandler(String Str){
		Str = Str.replace('a','A');
		Str = Str.replace('e','E');
		Str = Str.replace('i','I');
		Str = Str.replace('o','O');
		Str = Str.replace('u','U');
		return Str;
	}
}